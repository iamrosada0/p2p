generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../src/infra/database/sql/dev.db"
}

model User {
  uuid      String  @id @unique
  password  String
  email     String? @unique
  createdAt String
  updatedAt String
}
